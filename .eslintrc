{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": false,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "no-unused-vars": ["error", {"vars": "all", "varsIgnorePattern": "(React|App)"}]
  },
}

